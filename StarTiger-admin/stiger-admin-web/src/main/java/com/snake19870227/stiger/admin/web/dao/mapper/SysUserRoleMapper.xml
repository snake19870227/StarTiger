<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.snake19870227.stiger.admin.web.dao.mapper.SysUserRoleMapper">
    <resultMap id="base" type="com.snake19870227.stiger.admin.web.entity.po.SysUserRole">
        <id column="user_role_id" property="userRoleId" />
        <result column="user_id" property="userId" />
        <result column="role_id" property="roleId" />
        <result column="role_code" property="roleCode" />
    </resultMap>

    <select id="queryByUserId" resultMap="base">
        select * from sys_user_role where user_id = #{userId}
    </select>
</mapper>